    "keywords": [
        "unbxd",
        "search library",
        "ecommerce search"
    ],
    "author": "Unbxd",
    "license": "ISC",
    "dependencies": {
        "@unbxd-ui/unbxd-search-core": "0.5.4",
        "babel-polyfill": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "element-dataset": "^2.2.6"
    },
    "lint-staged": {
        "src/**/*.+(js|json|ts|tsx)": [
            "eslint --no-verify"
        ],
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    }
