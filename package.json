        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@babel/preset-react": "^7.10.4",
        "add-asset-html-webpack-plugin": "^3.1.3",
        "autoprefixer": "^9.8.6",
        "babel-loader": "^8.1.0",
